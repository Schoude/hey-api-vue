{
  "openapi": "3.1.1",
  "info": {
    "title": "Wave.Service.ClientPortal.Import.Api | v1",
    "version": "1.0.0"
  },
  "paths": {
    "/import/files": {
      "get": {
        "tags": [
          "Import"
        ],
        "operationId": "GetFiles",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ImportFile"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/import/from-excel": {
      "post": {
        "tags": [
          "Import"
        ],
        "operationId": "FromExcel",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "files"
                ],
                "type": "object",
                "properties": {
                  "files": {
                    "$ref": "#/components/schemas/IFormFileCollection"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "IFormFile": {
        "type": "string",
        "format": "binary"
      },
      "IFormFileCollection": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/IFormFile"
        }
      },
      "ImportFile": {
        "required": [
          "id",
          "originalFileName",
          "uniqueFileName",
          "status"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "originalFileName": {
            "type": "string"
          },
          "uniqueFileName": {
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/ImportStatus"
          }
        }
      },
      "ImportStatus": {
        "enum": [
          "pending",
          "processing",
          "completed",
          "failed"
        ],
        "type": "string"
      }
    }
  },
  "tags": [
    {
      "name": "Import"
    }
  ]
}